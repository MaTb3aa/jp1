int arr[1000];

int main()

{

    zeyad();

    ll n, a , b ; cin>>n;

     vector<int>v(n);

     for (int i= 0 ; i<n ; i++)

        cin>>v[i];

        sort(v.begin(),v.end());

     cin>>a;

     while(a--){

        cin>>b;

         vector<int>::iterator x;

         x=upper_bound(v.begin(),v.end(),b);

        cout<<(x-v.begin())<<el;

     }

}

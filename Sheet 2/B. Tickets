#include <bits/stdc++.h>

using namespace std;

#define ll long long

#define loop(n)       for(int i = 0; i < (n); i++)

#define lp(x, s, e)   for(int x = s; x < (e); x++)

#define lpe(x, s, e)  for(int x = s; x <= (e); x++)

#define  mp make_pair

#define  pb push_back

#define  all(a)   a.begin(), a.end()

#define  clr(x, val) memset((x), (val), sizeof(x))

#define  el  '\n'

void zeyad()

{

    ios_base::sync_with_stdio(0);

    cin.tie(0);

    cout.tie(0);

#ifndef ONLINE_JUDGE

    //freopen("input.txt", "r", stdin);

#endif // ONLINE_JUDGE

}

int arr[1000];

int main()

{

    zeyad();

    ll n, a , b ; cin>>n;

    queue <int>q;

    while (n--)

    {

        cin>>a>>b;

        if (a==1)

        {

            q.push(b);

        }

        else

        {

            if (q.front()==b)

                cout<<"Yes"<<el;

            else

                cout<<"No"<<el;

            q.pop();

        }

    }

}
